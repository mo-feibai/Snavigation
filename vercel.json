{
  "rewrites": [
    {
      "source": "/wallpaperApi/:match*",
      "destination": "https://wallhaven.cc/:match*"
    },
    {
      "source": "/geoApi/:match*",
      "destination": "https://geoapi.qweather.com/:match*"
    },
    {
      "source": "/weatherApi/:match*",
      "destination": "https://devapi.qweather.com/:match*"
    },
    {
      "source": "/iconApi/:match*",
      "destination": "https://www.google.com/:match*"
    },
    {
      "source": "/hitokotoApi/:match*",
      "destination": "https://international.v1.hitokoto.cn/:match*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "*.gstatic.com"
        }
      ],
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
        }
      ]
    }
  ]
}
